{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "include": [
    "./build/types/**/*.ts",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "src/**/*",
    "src/custom.d.ts",
    "./out/types/**/*.ts"
  ],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "composite": true,
    "target": "esnext",
    "lib": [
      "ES2023",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    },
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
